<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="codingpark.net.cheesecloud.view.SelectPathActivity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UserLogin"
        android:id="@+id/test_userLogin_bt"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CheckedFileInfo"
        android:id="@+id/test_checkedFileInfo_bt"
        android:layout_below="@+id/test_userLogin_bt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UploadFile"
        android:id="@+id/test_uploadFile_bt"
        android:layout_below="@+id/test_checkedFileInfo_bt"
        android:layout_alignRight="@+id/test_checkedFileInfo_bt"
        android:layout_alignEnd="@+id/test_checkedFileInfo_bt" />

</RelativeLayout>
